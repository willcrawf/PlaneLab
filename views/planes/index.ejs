<%- include('../partials/header') %>

  <table id='list' class = "table">
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Departs</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% planes.forEach((m) => { %>
        <tr>
          <td><%= m.airline %></td>
          <td><%= m.airport %></td>
          <td><%= m.departs %></td>
          <td><a href="/planes/<%= m._id %>">DETAILS</a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>

  <%- include('../partials/footer') %>